{
  "name": "bill-frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env VITE_PORT=4000 vite --host 0.0.0.0 --port 3231 --mode dev",
    "build": "tsc && vite build --mode prod",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "lint": "eslint src/**/*.{tsx,ts,js,jsx} --fix",
    "prettier": "prettier --config .prettierrc.js --write src/**/*.{tsx,ts,js,jsx}",
    "postinstall": "husky install && chmod 700 .husky/pre-commit"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "antd-mobile": "^5.0.0-rc.17",
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "i18next": "^21.6.6",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "*",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-redux": "^7.1.22",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.1",
    "c8": "^7.11.0",
    "classnames": "^2.3.1",
    "classnames-es-ts": "^2.2.7",
    "commitizen": "^4.2.4",
    "cypress": "^9.2.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.46.0",
    "tailwindcss": "^3.0.11",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitest": "0.0.131"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
